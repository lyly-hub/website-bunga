    "configurations": [
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:3000",
            "webRoot": "file:c://belajar 3/website",
            "port": 9222,
            "runtimeExecutable": "C:\\Program Files (x86)\\Microsoft\\Edge\\Application\\msedge.exe",
            "runtimeArgs": [
                "--remote-debugging-port=9222",
                "--user-data-dir=c:\\belajar 3\\website\\msedge"
            ],
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "file c:/belajar 3/website/src/*",
                "webpack:///./*": "file:///c:/belajar 3/website/*",
                "webpack:///*": "file:///c:/belajar 3/website/*",

            }

        },
        
        {
            "name": "Attach to Edge",
            "port": 9222,
            "request": "attach",
            "type": "msedge",
            "webRoot": "file c:/belajar 3/website/tokbung.html",
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "file c:/belajar 3/website/src/*",
                "webpack:///./*": "file:///c:/belajar 3/website/*",
                "webpack:///*": "file:///c:/belajar 3/website/*"
                // "webpack:///./src/*": "c:/belajar 3/website/src/*",
                // "webpack:///./*": "c:/belajar 3/website/*",  }
            },
        },

        {
            "name": "Attach to Edge",
            "port": 9222,
            "request": "attach",
            "type": "msedge",
            "webRoot": "${workspaceFolder}"
        },
        
        {
            "type": "msedge",
            "request": "launch",
